{
  "name": "20-devlr",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server/index.js",
    "start:dev": "concurrently \"nodemon server/index.js\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint '*/**/*.js' --ignore-path .gitignore",
    "server": "nodemon server/index.js",
    "seed": "node seeders/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandyquinlan/devlr.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "extends": "@imaginary-cloud/react",
    "root": true,
    "rules": {
      "react/prop-types": "off",
      "no-underscore-dangle": "off",
      "no-unused-vars": "off",
      "no-console": "off"
    }
  },
  "prettier": "@imaginary-cloud/prettier-config",
  "bugs": {
    "url": "https://github.com/brandyquinlan/devlr/issues"
  },
  "homepage": "https://github.com/brandyquinlan/devlr#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.12.7",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@imaginary-cloud/eslint-config-react": "^1.0.1",
    "concurrently": "^6.0.2",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^2.5.1",
    "prettier": "^2.3.0"
}
}